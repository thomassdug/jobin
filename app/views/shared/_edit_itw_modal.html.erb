<div class="modal fade" id="EditItwModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Edit my notes</h5>
         <%=link_to apply_path(apply), class:"quitmodal" do %>
        <i class="fas fa-times "></i>
          <%  end %>
      </div>
              <%= simple_form_for([apply, interview], html: {method: :patch}) do |f| %>

      <div class="modal-body">
      <hr>
      <%= f.input :notes, as: :text, input_html: { autocomplete: "notes" }%>
      </div>
      <div class="modal-footer">
           <%= f.button :submit, "Edit", class: "btn button-cta" %>
      </div>
    <% end %>
    </div>
  </div>

</div>
