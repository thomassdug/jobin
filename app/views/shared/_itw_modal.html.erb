
<div class="modal fade" id="ItwModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true" data-display="<%=display_modal ? "show" : "none" %>">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Add an Interview</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%= simple_form_for([apply, interview]) do |f| %>
      <hr>
      <%= f.input :start_date, as: :string, input_html: {id: "month_date"}%>
      <%= f.input :end_date, as: :string, input_html: {id: "month_date"}%>
      <%= f.input :step, as: :string%>
      <%= f.input :notes, as: :text%>
      </div>
      <div class="modal-footer">
           <%= f.button :submit, "Add", class: "btn button-cta"%>
      </div>
    </div>
  </div>
      <% end %>
</div>
